  "resolutions": {
    "babel-loader": "8.2.2",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pre-push": {
    "run": [
      "lint",
      "format"
    ]
  },
  "dependencies": {
    "@rollup/plugin-image": "^2.1.1",
    "babel-loader": "8.2.3",
    "react-helmet": "^6.1.0"
